% test dictionary select operator

(psconfig.ps) run

(lib/psunit.ps) require
(lib/collection.ps) require
(lib/math.ps) require
(lib/log.ps) require

/container <<
    /numbers <<
	1 (one)
	2 (zwei)
	3 (trois)
	4 (quattro)
	5 (cinque)
	6 (six)
	7 (sieben)
    >>
>> def

container /numbers get logln
container /numbers get dict.revert assert.TRUE
/srebmun exch def

srebmun logln
container /numbers get srebmun dict.eq assert.TRUE

showstack
psunit.summary

